<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link rel="shortcut icon" href="@Url.Content("~/Content/favicon.ico")" />
    <link rel="stylesheet" href="~/Content/bootstrap.css" />
    <link rel="stylesheet" href="~/Content/bootstrap-theme.css" />
    <link rel="stylesheet" type="text/css" href="~/Content/HomePageCSS.css" />
    <link rel="stylesheet" type="text/css" href="~/Content/NavCSS.css" />
    <title>@ViewBag.Title</title>
</head>
<body>
    <div id="completePage">
        <img src="~/Content/EquipmentSmall.JPG" />
    </div>
    <div id="page">
        @Html.Partial("_NavTop")
        @Html.Partial("_ButtonBand")
        @Html.Partial("_NavBarBottom")
        @Html.Partial("_Modal")
        <script src="~/Scripts/jquery-2.1.4.min.js"></script>
        <script src="~/Scripts/jquery-ui.min.js"></script>
        <script src="~/Scripts/bootstrap.min.js"></script>
        <div class="text-left" id="subscribeText">
            <a href="http://goo.gl/forms/jLZkcuX7c3" target="_blank">
                <div class="text-center">
                    Stay informed!
                    <br />
                    Subscribe to updates
                </div>
            </a>
        </div>
        <div class="text-center" id="socialMediaText">
            Check us out!
            <br />
            Follow us!
        </div>
        <div class="container" id="pageContainer">


            <div class="panel" id="pagePanel">
                <div class="panel-body" id="panelContent">
                    @RenderBody()
                </div>
            </div>
        </div>

    </div>
</body>
</html>
